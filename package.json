{
  "name": "instabytes-frontend",
  "version": "2.0.0",
  "description": "Frontend React para InstaBytes - Clone do Instagram com IA",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "start": "vite preview --port 8000",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript-eslint": "^8.1.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react",
    "instagram",
    "social-media",
    "image-upload",
    "ai",
    "gemini"
  ],
  "author": {
    "name": "Desenvolvedor",
    "email": "dev@exemplo.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Esousa97/instabytes-frontend"
  }
}
